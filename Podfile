# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'ExploreCocoaPods' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ExploreCocoaPods
  pod 'AFNetworking'
  pod 'SDWebImage'

  target 'ExploreCocoaPodsTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Kiwi'
  end

  target 'ExploreCocoaPodsUITests' do
    # Pods for testing
  end
end
